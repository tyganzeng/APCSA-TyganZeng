import java.util.Arrays;
import java.util.Scanner;
import static java.lang.System.*;

public class FancyWord
{
	private char[][] mat;

	public FancyWord()
	{

	}

   public FancyWord(String s)
	{
	   mat = new char[s.length()][s.length()];
	   Arrays.fill(mat, " ");
	   
	}

	public String toString()
	{
		String output="";





		return output+"\n\n";
	}
}